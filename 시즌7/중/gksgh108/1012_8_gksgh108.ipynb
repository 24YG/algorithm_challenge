N, M = map(int, input().split())
trees = list(map(int, input().split()))

start, end = 1, max(trees) 

while start <= end: 
    mid = (start+end)//2 

    logs = sum([tree-mid if tree-mid > 0 else 0 for tree in trees]) 
    
    if logs >= M: 
        start = mid + 1
    else:
        end = mid - 1

print(end)
